    "def crop_image(row):\n",
    "    \"\"\"Crop image based on coordinates.\n",
    "    \"\"\"\n",
    "    \n",
    "    file_name = row['filename']\n",
    "    image = cv2.imread(f'{MAIN_FILE_PATH}/{file_name}')\n",
    "    \n",
    "    x1 = round(row['x1'])\n",
    "    y1 = round(row['y1'])\n",
    "    x2 = round(row['x2'])\n",
    "    y2 = round(row['y2'])\n",
    "    \n",
    "    cropped_image = image[y1:y2, x1:x2]\n",
    "    \n",
    "    return cropped_image"
   ]
